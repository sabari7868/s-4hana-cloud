{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/resources/(.*)$",
      "target": "https://ui5.sap.com/resources/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none",
      "cacheControl": "public, max-age=604800"
    },
    {
      "source": "^/sap/opu/odata/sap/ESHOP_CART_SRV/(.*)$",
      "target": "/sap/opu/odata/sap/ESHOP_CART_SRV/$1",
      "destination": "S4HANA_SHOPPING",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}